<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="op_health_1" model="op.health">
            <field name="type">student</field>
            <field name="height">5.5</field>
            <field name="weight">50</field>
            <field name="blood_group">A+</field>
            <field name="eyeglasses" eval="True" />
            <field name="eyeglasses_no">2</field>
            <field name="regular_checkup" eval="True" />
            <field name="physical_challenges" eval="False" />
            <field name="physical_challenges_note"></field>
