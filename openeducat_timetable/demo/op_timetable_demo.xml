<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="op_session_1" model="op.session">
            <field name="timing_id" ref="op_timing_1"/>
            <field name="faculty_id" ref="openeducat_core.op_faculty_1"/>
            <field name="course_id" ref="openeducat_core.op_course_2"/>
            <field name="batch_id" ref="openeducat_core.op_batch_1"/>
            <field name="subject_id" ref="openeducat_core.op_subject_6"/>
            <field name="start_datetime" eval="time.strftime('%Y-%m-%d 04:30:00')"/>
            <field name="end_datetime" eval="time.strftime('%Y-%m-%d 05:30:00')"/>
        </record>

        <record id="op_session_2" model="op.session">
            <field name="timing_id" ref="op_timing_2"/>
            <field name="faculty_id" ref="openeducat_core.op_faculty_3"/>
            <field name="course_id" ref="openeducat_core.op_course_3"/>
            <field name="batch_id" ref="openeducat_core.op_batch_2"/>
            <field name="subject_id" ref="openeducat_core.op_subject_7"/>
            <field name="start_datetime" eval="time.strftime('%Y-%m-%d 05:30:00')"/>
            <field name="end_datetime" eval="time.strftime('%Y-%m-%d 06:30:00')"/>
        </record>

        <record id="op_session_3" model="op.session">
            <field name="timing_id" ref="op_timing_4"/>
            <field name="faculty_id" ref="openeducat_core.op_faculty_4"/>
            <field name="course_id" ref="openeducat_core.op_course_5"/>
            <field name="batch_id" ref="openeducat_core.op_batch_4"/>
            <field name="subject_id" ref="openeducat_core.op_subject_8"/>
            <field name="start_datetime" eval="time.strftime('%Y-%m-%d 05:30:00')"/>
            <field name="end_datetime" eval="time.strftime('%Y-%m-%d 06:30:00')"/>
        </record>

        <record id="op_session_4" model="op.session">
            <field name="timing_id" ref="op_timing_3"/>
            <field name="faculty_id" ref="openeducat_core.op_faculty_6"/>
            <field name="course_id" ref="openeducat_core.op_course_4"/>
