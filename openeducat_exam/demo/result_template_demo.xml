<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="op_result_template_1" model="op.result.template">
            <field name="name">BOA Template</field>
            <field name="result_date" eval='(DateTime.today())' />
            <field name="exam_session_id" ref="op_exam_session_1" />
            <field name="criteria_ids"
                eval="[(6,0,[ref('op_min_clear_criteria_1'),ref('op_min_clear_criteria_2'),ref('op_min_clear_criteria_3'),ref('op_min_clear_criteria_4')])]" />
            <field name="pass_status_ids"
                eval="[(6,0,[ref('pass_status_1'),ref('pass_status_2'),ref('pass_status_3'),ref('pass_status_4')])]" />
        </record>

        <record id="op_result_template_2" model="op.result.template">
            <field name="name">LRTP Template</field>
            <field name="result_date" eval='(DateTime.today())' />
            <field name="exam_session_id" ref="op_exam_session_2" />
            <field name="criteria_ids"
                eval="[(6,0,[ref('op_min_clear_criteria_1'),ref('op_min_clear_criteria_2'),ref('op_min_clear_criteria_3'),ref('op_min_clear_criteria_4')])]" />
            <field name="pass_status_ids"
                eval="[(6,0,[ref('pass_status_1'),ref('pass_status_2'),ref('pass_status_3'),ref('pass_status_4')])]" />
        </record>
    </data>
</odoo>